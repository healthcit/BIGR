CREATE OR REPLACE FUNCTION GET_DESC_FROM_DX_TC_HIERARCHY(
  pOwnerCode IN PDC_LOOKUP.LOOKUP_TYPE_CD%type) return varchar2 Is
  vDesc PDC_LOOKUP.LOOKUP_TYPE_CD_DESC%type;
  BEGIN
  	SELECT LOOKUP_TYPE_CD_DESC INTO vDesc
  	FROM PDC_LOOKUP WHERE LOOKUP_TYPE_CD = pOwnerCode;
  	return vDesc;
  EXCEPTION
     WHEN NO_DATA_FOUND THEN RETURN '';
     WHEN OTHERS THEN RETURN '';
END;
/


CREATE OR REPLACE TRIGGER ILTDS_ACCT_BX_LAYOUT_PK_TRG
BEFORE INSERT
ON ILTDS_ACCOUNT_BOX_LAYOUT
REFERENCING NEW AS NEW OLD AS OLD
FOR EACH ROW
DECLARE
  v_seq_val NUMBER;
BEGIN
  IF :new.id IS NULL
  THEN
    SELECT ILTDS_ACCOUNT_BOX_LAYOUT_SEQ.nextval INTO v_seq_val FROM dual;
	:new.id := v_seq_val;
  END IF;
EXCEPTION
  WHEN others THEN
    raise_application_error(-20001, 'Primary key violation on ILTDS_ACCOUNT_BOX_LAYOUT: '||SQLERRM);
END ILTDS_ACCT_BX_LAYOUT_PK_TRG;
/
